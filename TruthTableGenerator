def truthTable(n: int) -> list[bool]:
    def table(n):
        if n:
            for i in TF:
                r.append(i)
                table(n-1)
                r.pop()
        else:
            t.append(r.copy())

    r = []
    t = []
    TF = (True, False)

    table(n)

    return t
